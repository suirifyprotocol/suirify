# Sui Network Configuration
SUI_NETWORK=devnet
SUI_RPC=https://fullnode.devnet.sui.io:443

# Protocol Configuration
PACKAGE_ID=
ADMIN_CAP_ID=
PROTOCOL_CONFIG_ID=
ATTESTATION_REGISTRY_ID=
JURISDICTION_POLICY_ID=

# Admin/Sponsor Key (NOT the enclave's private key)
# This is used to pay gas fees for transactions
ADMIN_PRIVATE_KEY=

# Mint Fee Configuration
MINT_FEE=

# Server Configuration
PORT=4000
HOST=0.0.0.0
REQUEST_BODY_LIMIT=25mb

# Security Configuration
SECRET_PEPPER=
ADMIN_API_KEY=
BYPASS_FACE_MATCH=false

# Nautilus Enclave Configuration (Optional)
# If these are set, the backend will use secure enclave-based minting
# If not set, it will fall back to legacy direct minting
ENCLAVE_CID=3
ENCLAVE_PORT=5000
ENCLAVE_CONFIG_ID=
ENCLAVE_OBJECT_ID=
